<UserControl x:Class="Whisper_Messenger.Components.BindablePasswordBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Whisper_Messenger.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             >
    <PasswordBox x:Name="Password_textBox" PasswordChanged="Password_textBox_PasswordChanged"
                 Style="{DynamicResource PasswordBoxStyle1}"/>
</UserControl>
